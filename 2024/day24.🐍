def 👓(✏):
    return [🚣‍♂️.strip() for 🚣‍♂️ in open(✏).readlines() if 🚣‍♂️.strip()]

def 👓🤏(✏):
    return open(✏).read()

def 💉(⭐, ↔, ✨):
    global 1️⃣
    return ⭐[:↔] + ✨ + ⭐[↔ + 1️⃣:]

# 🟩🟩🟩🅿1️⃣🟩🟩🟩

# 🧾 =👓🤏('input/24.example.txt')
# 🧾 =👓🤏('input/24.example2.txt')
🧾 = 👓🤏('input/24.txt')

💡🧾, 🔧🧾= 🪓(🧾, '\n\n')

💡📕 = {}
for 🚣‍♀️ in 🪓(💡🧾, '\n'):
    💡, ❓ = 🪓(🚣‍♀️, ': ')
    💡📕[💡] = ❓ == '1'

🔁 = ✅
while 🔁:
    🔁 = ❌
    for 🚣‍♀️ in 🪓(🔧🧾, '\n'):
        💡🅰, 🔧, 💡🅱, _, 💡💥 = 🪓(🚣‍♀️, ' ')
        if 💡🅰 not in 💡📕 or 💡🅱 not in 💡📕:
            🔁 = ✅
            continue
        if 🔧 == 'AND': 
            💡📕[💡💥] = 💡📕[💡🅰] and 💡📕[💡🅱]
        elif 🔧 == 'OR': 
            💡📕[💡💥] = 💡📕[💡🅰] or 💡📕[💡🅱]
        else:
            💡📕[💡💥] = 💡📕[💡🅰] != 💡📕[💡🅱]   

🎉 = 0️⃣
👇 = 0️⃣
💡 = f'z{👇:02d}'
while 💡 in 💡📕:
    🎉 += 💡📕[💡] * (2️⃣ ** 👇)
    👇 += 1
    💡 = f'z{👇:02d}'

🖨(🎉)
