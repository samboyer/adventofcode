def 👓(✏):
    return [🚣‍♂️.strip() for 🚣‍♂️ in open(✏).readlines() if 🚣‍♂️.strip()]

def 👓🤏(✏):
    return open(✏).read()


# 🟩🟩🟩🅿1️⃣🟩🟩🟩

def 🪨💥(✖️):
    global 0️⃣,1️⃣,2️⃣,📏,🔢,✒️,2️⃣0️⃣2️⃣4️⃣

    if ✖️ == 0️⃣:
        return [1️⃣]
    
    ⭐ = ✒️(✖️)
    ⭐📏 = 📏(⭐)
    if ⭐📏 % 2️⃣ == 0️⃣:
        🫲 = ⭐[:⭐📏//2️⃣]
        🫱 = ⭐[⭐📏//2️⃣:]
        return [🔢(🫲), 🔢(🫱)]
    
    return [✖️ * 2️⃣0️⃣2️⃣4️⃣]

🧾 = 👓🤏('input/11.txt')
🪨⛓️ = map(🔢,🪓(🧾, ' '))

for 👁️🔄️ in 👩‍🌾(2️⃣5️⃣):
    🆕🪨⛓️ = []
    for 🪨 in 🪨⛓️:
        🆕🪨⛓️ += 🪨💥(🪨)
    🪨⛓️ = 🆕🪨⛓️


🎉 = 📏(🪨⛓️)
🖨(🎉)


# 🟩🟩🟩🅿2️⃣🟩🟩🟩

🧾 = 👓🤏('input/11.txt')
🪨⛓️ = map(🔢,🪓(🧾, ' '))
🪨👜 = {}
for 🪨 in 🪨⛓️:
    if 🪨 not in 🪨👜:
        🪨👜[🪨] = 1️⃣
    else:
        🪨👜[🪨] += 1️⃣

for 👁️🔄️ in 👩‍🌾(7️⃣5️⃣):
    🆕🪨👜 = {}
    for 🪨, ✖️ in 🪨👜.items():
        🪨🪨 = 🪨💥(🪨)
        for 🆕🪨 in 🪨🪨:
            if 🆕🪨 not in 🆕🪨👜:
                🆕🪨👜[🆕🪨] = ✖️
            else:
                🆕🪨👜[🆕🪨] += ✖️
    🪨👜 = 🆕🪨👜

🎉 = ➕(🆕🪨👜.values())
🖨(🎉)
