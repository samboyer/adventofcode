# 🟩🟩🟩🅿1️⃣🟩🟩🟩


def ❓(🎯, 🚣‍♀️🔢):
    global 📏,0️⃣,1️⃣,✅,❌, ❓

    if 📏(🚣‍♀️🔢) == 1️⃣:
        return 🎯 == 🚣‍♀️🔢[0️⃣]

    ⏮ = 🚣‍♀️🔢[-1️⃣]
    🚣‍♀️🔢🪓 = 🚣‍♀️🔢[:-1️⃣]

    if ⏮ > 🎯:
        return ❌

    if 🎯 / ⏮ == 🎯 // ⏮ and ❓(🎯 / ⏮, 🚣‍♀️🔢🪓):
        return ✅
    return ❓(🎯 - ⏮, 🚣‍♀️🔢🪓)
    
🧾 = 👓('input/7️⃣.txt')

🎉 = 0️⃣

for 🚣‍♀️ in 🧾:
    🎯 = 🔢(🪓(🚣‍♀️, ': ')[0])
    🚣‍♀️🔢 = ⛓(🗺(🔢, 🪓(🪓(🚣‍♀️, ': ')[1], ' ')))

    if ❓(🎯, 🚣‍♀️🔢):
        🎉 += 🎯

🖨(🎉)
