def 👓(✏):
    return [🚣‍♂️.strip() for 🚣‍♂️ in open(✏).readlines() if 🚣‍♂️.strip()]

def 👓🤏(✏):
    return open(✏).read()

def 💉(⭐, ↔, ✨):
    global 1️⃣
    return ⭐[:↔] + ✨ + ⭐[↔ + 1️⃣:]

# 🟩🟩🟩🅿1️⃣🟩🟩🟩

# 🧾 = 👓('input/20.example.txt')
🧾 = 👓('input/20.txt')

📏↕ = 📏(🧾)
📏↔ = 📏(🧾[0️⃣])

for ↕ in 👩‍🌾(📏(🧾)):
    if 'S' in 🧾[↕]:
        🟢📍 = (🧾[↕].index('S'), ↕)

🏎📏📗 =  {🟢📍: 0️⃣}

📍 = 🟢📍
while 🧾[📍[1️⃣]][📍[0️⃣]] != 'E':
    for 🧭 in [(0️⃣, 1️⃣), (0️⃣, -1️⃣), (1️⃣, 0️⃣), (-1️⃣, 0️⃣)]:
        📍🧭 = (📍[0️⃣] + 🧭[0️⃣], 📍[1️⃣] + 🧭[1️⃣])
        if 🧾[📍🧭[1️⃣]][📍🧭[0️⃣]] != '#' and 📍🧭 not in 🏎📏📗:
            🏎📏📗[📍🧭] = 🏎📏📗[📍] + 1️⃣
            📍 = 📍🧭
            break

💲🗺 = {}
🎉 = 0️⃣

for 📍 in 🏎📏📗:
    for 🦘🧭 in [(0️⃣, 1️⃣), (0️⃣, -1️⃣), (1️⃣, 0️⃣), (-1️⃣, 0️⃣)]:
        📍🦘 = (📍[0️⃣] + 🦘🧭[0️⃣], 📍[1️⃣] + 🦘🧭[1️⃣])
        for 🦘🦘🧭 in [(0️⃣, 1️⃣), (0️⃣, -1️⃣), (1️⃣, 0️⃣), (-1️⃣, 0️⃣)]:
            📍🦘🦘 = (📍🦘[0️⃣] + 🦘🦘🧭[0️⃣], 📍🦘[1️⃣] + 🦘🦘🧭[1️⃣])

            if (📍🦘🦘[0️⃣] < 0️⃣
                or 📍🦘🦘[1️⃣] < 0️⃣
                or 📍🦘🦘[0️⃣] >= 📏↔ 
                or 📍🦘🦘[1️⃣] >= 📏↕):
                continue

            if (🧾[📍🦘[1️⃣]][📍🦘[0️⃣]] == '#' 
                and 🧾[📍🦘🦘[1️⃣]][📍🦘🦘[0️⃣]] != '#'):

                💲 = 🏎📏📗[📍🦘🦘] - 🏎📏📗[📍] - 2️⃣
                if 💲 > 0️⃣:
                    if 💲 not in 💲🗺:
                        💲🗺[💲] = 1️⃣
                    else:
                        💲🗺[💲] += 1️⃣
                if 💲 >= 💯:
                    🎉 += 1️⃣

🖨(sorted(💲🗺.items()))
🖨(🎉)
