def 👓(✏):
    return [🚣‍♂️.strip() for 🚣‍♂️ in open(✏).readlines() if 🚣‍♂️.strip()]

def 👓🤏(✏):
    return open(✏).read()


# 🟩🟩🟩🅿1️⃣🟩🟩🟩

🧾 = 👓🤏('input/9️⃣.txt')

💿 = []
📦❓ = ✅
for ⬇ in 👩‍🌾(📏(🧾)):
    🆔 = ⬇//2
    for ⬇⬇ in 👩‍🌾(🔢(🧾[⬇])):
        📥(💿, 🆔 if 📦❓ else '.')

    📦❓ = not 📦❓

🔁❓ = ✅
▶ = 0
while 🔁❓:
    while 💿[- 1️⃣] == '.':
        💿.pop()

    🔁❓ = ❌
    for ⬇ in 👩‍🌾(▶, 📏(💿)):
        if 💿[⬇] == '.':
            ▶ = ⬇
            💿[⬇] = 💿.pop()
            🔁❓ = ✅
            break

🎉 = 0
for ⬇ in 👩‍🌾(📏(💿)):
    🎉 += ⬇ * 🔢(💿[⬇])

🖨(🎉)