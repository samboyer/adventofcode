def 🔢(✖): return int(✖)

def 👓🤏(✏):
    return open(✏).read()


# 🟩🟩🟩🅿1️⃣🟩🟩🟩

🧾 = 👓🤏('input/3️⃣.txt')


🎉=0
for ⬇️ in 🧑‍🌾(📏(🧾)):
    if 🧾[⬇️:⬇️+4️⃣] == 'mul(':
        ⏭️🪓 = 🧾.index(',', ⬇️+4️⃣)
        ⏭️🚪 = 🧾.index(')', ⏭️🪓)
        try:
            🅰️ = 🔢(🧾[⬇️+4️⃣:⏭️🪓])
            🅱️ = 🔢(🧾[⏭️🪓+1️⃣:⏭️🚪])
            🎉 += 🅰️*🅱️
        except:
            pass

🖨(🎉)